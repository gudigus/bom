<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config
3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- iron -->
		<typeAlias alias="Iron_User_Info"
			type="com.spring.bom.model.iron.User_Info" />
		<typeAlias alias="Iron_Board"
			type="com.spring.bom.model.iron.Board" />
		<typeAlias alias="Iron_Follow"
			type="com.spring.bom.model.iron.Follow" />
		<typeAlias alias="Iron_HashTag"
			type="com.spring.bom.model.iron.HashTag" />
		<typeAlias alias="Iron_Like_Bookmark"
			type="com.spring.bom.model.iron.Like_Bookmark" />

		<!-- bro -->
		<typeAlias type="com.spring.bom.model.bro.User_info"
			alias="User_info" />
		<typeAlias type="com.spring.bom.model.bro.Interest"
			alias="Interest" />

		<!-- god -->
		<typeAlias type="com.spring.bom.model.god.JHBoard"
			alias="GodBoard" />
		<typeAlias type="com.spring.bom.model.god.JHVote"
			alias="GodVote" />

		<!-- bear -->
		<typeAlias type="com.spring.bom.model.bear.Chat"
			alias="Chat" />

		<typeAlias type="com.spring.bom.model.bear.User"
			alias="User" />

		<!-- coffee -->
		<typeAlias
			type="com.spring.bom.model.coffee.CoffeeUser_info"
			alias="CoffeeUser_info" />
		<typeAlias type="com.spring.bom.model.coffee.BoardUser_info"
			alias="CoffeeBoardUser_info" />
		<typeAlias
			type="com.spring.bom.model.coffee.ReportUser_infoBoard"
			alias="CoffeeReportUser_infoBoard" />

		<!-- hoon -->
		<typeAlias type="com.spring.bom.model.hoon.Junghun"
			alias="Junghun" />
	</typeAliases>
</configuration>